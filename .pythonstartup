# -*- encofing: utf-8 -*-
# startup spells
import sys
import os
import readline
import rlcompleter2
import atexit

# tab auto-complete
readline.parse_and_bind('tab: complete')
# historyfile
histfile = os.path.join(os.environ['HOME'], '.pythonhistory')
try:
    readline.read_history_file(histfile)
except IOError:
    pass
atexit.register(readline.write_history_file, histfile)
#del os, histfile, readline, rlcompleter2, atexit
