# -*- encofing: utf-8 -*-
# startup spells
import sys
import os
import readline
import atexit

try:
    import rlcompleter3
except ImportError:
    print "no rlcompleter3 module"

# tab auto-complete
readline.parse_and_bind('tab: complete')
# historyfile
histfile = os.path.join(os.environ['HOME'], '.pythonhistory')
try:
    readline.read_history_file(histfile)
except IOError:
    pass
atexit.register(readline.write_history_file, histfile)
del os, histfile, readline, rlcompleter3, atexit

